{
  "name": "campusreach",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:local": "mv .env.local .env.production.local 2>/dev/null; mv .env.development.local .env.local 2>/dev/null; next dev",
    "dev:prod": "mv .env.local .env.development.local 2>/dev/null; mv .env.production.local .env.local 2>/dev/null; next dev",
    "env:local": "mv .env.local .env.production.local 2>/dev/null; mv .env.development.local .env.local 2>/dev/null; echo 'Switched to local Supabase'",
    "env:prod": "mv .env.local .env.development.local 2>/dev/null; mv .env.production.local .env.local 2>/dev/null; echo 'Switched to production Supabase'",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "eslint",
    "postinstall": "prisma generate",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:pull": "prisma db pull",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "migrate:neon": "tsx scripts/migrate-data-from-neon.ts",
    "migrate:auth": "tsx scripts/migrate-auth-to-supabase.ts"
  },
  "dependencies": {
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "^7.2.0",
